<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<!--

    Configuration file for UrlRewriteFilter
    http://tuckey.org/urlrewrite/

-->
<urlrewrite>

	<rule>
        <from>^$</from>
        <to>/index.jsp</to>
    </rule>
    
    <rule>
        <from>^/contact$</from>
        <to>/contact.jsp</to>
    </rule>
    
    <rule>
        <from>^/contact/([a-z]+)$</from>
        <to type="redirect">/contact.jsp?enquiry=$1</to>
    </rule>
    
    <rule>
        <from>^/support$</from>
        <to>/support.jsp</to>
    </rule>
    
    <rule>
        <from>^/about-us$</from>
        <to>/about-us.jsp</to>
    </rule>
    
    <rule>
        <from>^/beta$</from>
        <to>/beta.jsp</to>
    </rule>
    
    <rule>
        <from>^/chess-chaps$</from>
        <to>/chess-mates.jsp</to>
    </rule>
    
     <rule>
        <from>^/leaderboards$</from>
        <to>/leaderboards.jsp</to>
    </rule>
    
    <rule>
        <from>^/privacy-policy$</from>
        <to>/privacy-policy.jsp</to>
    </rule>
    



</urlrewrite>

